@using BizData.Entities
@model IEnumerable<Provincia>

<section>
    <input type="text" id="myInput" onkeyup="myFunction()" placeholder="Search for names..">
</section>
<table id="myTable">
    <tr>
        <th>
            Provincia
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Nombre
            </td>
        </tr>
    }
</table>