@using BizData.Entities
@using BizLogic.Authentication
@model IEnumerable<Usuario>

<section>
    <input type="text" id="myInput" onkeyup="myFunction()" placeholder="Search for names..">
</section>
<table id="myTable">
    <tr>
        <th>
            Nombre
        </th>
        <th>
            Apellido
        </th>
        <th>
            Correo
        </th>
        <th>
            Unidad Organizativa
        </th>
        <th>
            
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.FirstName
            </td>
            <td>
                @item.FirstLastName
            </td>
            <td>
                @item.UserName
            </td>
            <td>
                @if(item.UnidadOrganizativa!=null)
                {
                    Html.DisplayText(item.UnidadOrganizativa.Nombre);

                }
            </td>
            <td>
                @Html.ActionLink("editar","EditUsuario","Admin", new RegisterUsuarioCommand { EditEmail=item.Email,Email=item.Email,FirstLastName=item.FirstLastName,FirstName=item.FirstName,SecondLastName=item.SecondLastName,SecondName=item.SecondName,Password= "P9n$"} )
            </td>
        </tr>
    }
</table>